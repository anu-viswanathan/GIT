<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Warranty_Remaining_Years__c</fullName>
    <description>Derived from Num_of_Years_Product_Used__c and Warranty_Term_Years__c. Has to be greater than or equal to 0.</description>
    <externalId>false</externalId>
    <formula>IF( Num_of_Years_Product_Used__c &lt;   Warranty_Term_Years__c  , Warranty_Term_Years__c - Num_of_Years_Product_Used__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Warranty Remaining (Years)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
