<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Plant_Period__c</fullName>
    <deleteConstraint>SetNull</deleteConstraint>
    <externalId>false</externalId>
    <inlineHelpText>Used to link the Case to the correct production period based on the Installation Date and Product&apos;s Iko Family, so the correct Plant can be associated</inlineHelpText>
    <label>Plant Period</label>
    <lookupFilter>
        <active>true</active>
        <booleanFilter>1 AND (2 OR 3)</booleanFilter>
        <errorMessage>Manufacturing Period does not align with Product Family and Installation Date</errorMessage>
        <filterItems>
            <field>$Source.Installation_Date__c</field>
            <operation>greaterOrEqual</operation>
            <valueField>Plant_Period__c.Start_Date__c</valueField>
        </filterItems>
        <filterItems>
            <field>Plant_Period__c.End_Date__c</field>
            <operation>equals</operation>
            <value></value>
        </filterItems>
        <filterItems>
            <field>$Source.Installation_Date__c</field>
            <operation>lessOrEqual</operation>
            <valueField>Plant_Period__c.Last_Date_of_Install__c</valueField>
        </filterItems>
        <isOptional>false</isOptional>
    </lookupFilter>
    <referenceTo>Plant_Period__c</referenceTo>
    <relationshipLabel>Cases</relationshipLabel>
    <relationshipName>Cases</relationshipName>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>true</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Lookup</type>
</CustomField>
