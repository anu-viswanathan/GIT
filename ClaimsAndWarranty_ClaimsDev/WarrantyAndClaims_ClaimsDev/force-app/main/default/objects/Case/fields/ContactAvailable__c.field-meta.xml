<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ContactAvailable__c</fullName>
    <description>Created for Customer Service Implementation. This is a flag to indicate whether a contact is associated to a case</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(ContactId), true, false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Contact Available</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
