<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CSDaysOpen__c</fullName>
    <description>Created for Customer Service Implementation, This will calculate the age of a case</description>
    <externalId>false</externalId>
    <formula>BLANKVALUE(DATEVALUE(ClosedDate), TODAY()) - DATEVALUE(CreatedDate)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Number of days a case has been opened</inlineHelpText>
    <label>Days Open</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
