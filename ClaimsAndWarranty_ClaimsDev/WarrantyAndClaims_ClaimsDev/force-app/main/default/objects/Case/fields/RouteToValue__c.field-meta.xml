<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RouteToValue__c</fullName>
    <description>Created for Customer Service Implementation. This is a flag to indicate whether the case is routed to a contact.</description>
    <externalId>false</externalId>
    <formula>OR((ISBLANK(RoutedTo__c)),(ISBLANK(TEXT(Department__c))),(ISPICKVAL(Route_Status__c,&quot;Email Sent&quot;)))</formula>
    <label>Router To Value</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
