<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tier_0_cannot_get_Coop_funds</fullName>
    <active>true</active>
    <errorConditionFormula>AND(OR(Text(Currency_Used__c)=&quot;Co-op $&quot;,Text(Currency_Used__c)=&quot;Premium Co-op $&quot;),
Text(Transaction_Type__c)=&quot;Credit&quot;,
Account_ID__r.Present_Tier_Formula__c=&quot;0&quot;,
Text(Transaction_Sources__c)!=&quot;Co-Op Reimbursement&quot;)</errorConditionFormula>
    <errorMessage>Tier 0 contractors are not eligible to get Coop funds.</errorMessage>
</ValidationRule>
