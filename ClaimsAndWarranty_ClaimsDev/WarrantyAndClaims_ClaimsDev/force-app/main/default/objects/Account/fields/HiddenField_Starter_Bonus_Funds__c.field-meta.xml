<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HiddenField_Starter_Bonus_Funds__c</fullName>
    <description>This field is getting used to calculate Coop amount based on Contractor&apos;s Tier. 500 $ for Tier 3, 300 $ for Tier 2, Nothing for Tier 1.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(Present_Tier_Formula__c)),
if(Present_Tier_Formula__c=&quot;2&quot;,300,
if(Present_Tier_Formula__c=&quot;3&quot;,500,0)),0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>HiddenField - Starter Bonus Funds</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
