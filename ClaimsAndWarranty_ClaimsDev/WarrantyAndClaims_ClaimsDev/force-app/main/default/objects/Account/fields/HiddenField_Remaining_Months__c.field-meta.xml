<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HiddenField_Remaining_Months__c</fullName>
    <description>This field is used to calculate the remaining months of a year.</description>
    <externalId>false</externalId>
    <formula>IF(Month(Today())==1, 12, 12-Month(Today()) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>HiddenField - Remaining Months</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
