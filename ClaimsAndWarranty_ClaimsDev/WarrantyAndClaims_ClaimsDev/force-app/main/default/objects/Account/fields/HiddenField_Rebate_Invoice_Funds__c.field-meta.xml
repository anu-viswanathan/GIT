<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HiddenField_Rebate_Invoice_Funds__c</fullName>
    <description>This field is getting used to calculate Rebate Coop funds based on Tier. 1% for Tier 3, 0.5% for Tier 2, Nothing for Tier 1.</description>
    <externalId>false</externalId>
    <formula>If(Present_Tier_Lookup__r.Name=&quot;Tier 2&quot;,0.005,if(Present_Tier_Lookup__r.Name=&quot;Tier 3&quot;,0.01,0))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>HiddenField - Rebate Invoice Funds</label>
    <precision>18</precision>
    <required>false</required>
    <scale>3</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
