<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Warranty_Term_Text__c</fullName>
    <description>Converts the warranty term to a written out number in prose. All caps for some reason. Do not use on layouts.</description>
    <externalId>false</externalId>
    <formula>Case(
      TEXT(Warranty_Term__c),
      &quot;5&quot;, &quot;FIVE&quot;,
      &quot;10&quot;, &quot;TEN&quot;,
      &quot;15&quot;, &quot;FIFTEEN&quot;,
      &quot;20&quot;, &quot;TWENTY&quot;,
      &quot;25&quot;, &quot;TWENTY FIVE&quot;,
      &quot;THIRTY&quot;
     )</formula>
    <label>Warranty Term Text</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
